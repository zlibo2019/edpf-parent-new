    <select id="selectListByKeys" parameterType="String" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from dt_user
        <where>
            <if test="keys != null">
                <bind name="keysLike" value="'%'+keys+'%'"/>
                (user_no like #{keysLike}
                or user_lname like #{keysLike}
                or user_depname like #{keysLike}
                or user_card like #{keysLike}
                or user_id like #{keysLike}
                or user_post like #{keysLike}
                or user_telephone like #{keysLike})
            </if>
            <![CDATA[
              and user_type < 51
            ]]>
            <if test="userDeps != null and userDeps != ''">
                and user_dep in
                <foreach collection="userDeps.split(',')" item="item" separator="," open="(" close=")">
                    #{item, jdbcType=INTEGER}
                </foreach>
            </if>
        </where>
        order by user_serial
    </select>
    <delete id="deleteBatchByKeys" parameterType="List">
        delete from
        dt_user
        where (user_serial) in
        <foreach close=")" collection="list" index="index" item="item" open="(" separator=",">
            (#{item.userSerial})
        </foreach>
    </delete>
