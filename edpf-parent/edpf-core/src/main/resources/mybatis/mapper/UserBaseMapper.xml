    <select id="selectVisUserByDept" parameterType="com.weds.edpf.core.entity.UserBaseEntity" resultMap="BaseResultMap">
        select
        t1.*, num
        <![CDATA[
        from dt_user t1
                 left join (select user_serial, count(1) as num
                            from Vis_register
                            group by user_serial) t2 on t1.user_serial = t2.user_serial
        where t1.user_type < 51
        ]]>
        <if test="userLname != null and userLname!= ''">
            <bind name="userLnameLike" value="'%'+userLname+'%'"/>
            and user_lname like #{userLnameLike}
        </if>
        <if test="keys != null and keys!= ''">
            <bind name="keysLike" value="'%'+keys+'%'"/>
            and (user_lname like #{keysLike} or user_no like #{keysLike})
        </if>
        <if test="userDeps != null and userDeps != ''">
            and user_dep in
            <foreach collection="userDeps.split(',')" item="item" separator="," close=")" open="(">
                #{item, jdbcType=INTEGER}
            </foreach>
        </if>
        order by num desc
    </select>
